<h1>Rental Orders</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Parkingaddress</th>
      <th>User</th>
      <th>Rentstart</th>
      <th>Rentend</th>
      <th>Ordertotal</th>
      <th>Orderstatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rental_orders_rent.each do |rental_order| %>
      <tr>
        <td><%= rental_order.id %></td>
        <td><%= rental_order.ParkingAddress_id %></td>
        <td><%= rental_order.user_id %></td>
        <td><%= rental_order.rentStart %></td>
        <td><%= rental_order.rentEnd %></td>
        <td><%= rental_order.orderTotal %></td>
        <td><%= rental_order.orderStatus %></td>
        <td><%= link_to 'Show', rental_order %></td>
        <td><%= link_to 'Edit', edit_rental_order_path(rental_order) %></td>
        <td><%= link_to 'Destroy', rental_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

